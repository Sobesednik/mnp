  "devDependencies": {
    "assert-diff": "1.2.4",
    "zoroaster": "0.4.6"
  },
  "dependencies": {
    "camel-case": "3.0.0",
    "catchment": "^1.0.0",
    "makepromise": "1.1.1",
    "reloquent": "0.2.0",
    "restream": "1.2.0",
    "rqt": "0.2.0",
    "spawncommand": "^1.1.0",
    "wrote": "1.2.0"
