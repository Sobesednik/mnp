{
  "name": "mnp",
  "version": "0.10.2",
  "description": "Quickly create a Node.js package with a Github repository.",
  "main": "build",
  "bin": "build/bin/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask -t 20000",
    "spec": "yarn t test/spec -t 20000",
    "mask": "yarn t test/mask -t 20000",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "d": "git add documentary README.md; git commit -m doc; git push;",
    "dev": "node src/bin/alamode",
    "start": "node build/bin",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "b": "alamode src -o build -i bin/alamode.js,bin/.eslintrc",
    "build": "yarn-s b doc"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnpjs/mnp.git"
  },
  "keywords": [
    "npm",
    "mnp",
    "new",
    "node",
    "node.js",
    "package",
    "create",
    "github",
    "git",
    "repo",
    "repository",
    "bootstrap",
    "scaffold",
    "development",
    "dev",
    "generate",
    "generator",
    "yeoman",
    "slush",
    "project",
    "init",
    "template",
    "modern"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mnpjs/mnp/issues"
  },
  "homepage": "https://github.com/mnpjs/mnp#readme",
  "devDependencies": {
    "@wrote/read": "1.0.0",
    "@wrote/rm": "1.1.0",
    "alamode": "1.5.1",
    "assert-diff": "2.0.3",
    "documentary": "1.20.1",
    "eslint-config-artdeco": "1.0.1",
    "snapshot-context": "2.0.4",
    "temp-context": "2.0.0",
    "yarn-s": "1.1.0",
    "zoroaster": "3.6.0"
  },
  "dependencies": {
    "@mnpjs/package": "1.7.1",
    "@mnpjs/structure": "1.1.1",
    "@rqt/github": "1.0.0",
    "africa": "1.2.0",
    "argufy": "1.2.1",
    "bosom": "1.0.0",
    "camel-case": "3.0.0",
    "erte": "1.1.4",
    "makepromise": "3.0.1",
    "mnp-idio": "1.2.5",
    "promto": "1.0.1",
    "reloquent": "1.0.6",
    "restream": "3.2.1",
    "rqt": "3.0.0",
    "spawncommand": "2.1.0",
    "usually": "1.0.0",
    "wrote": "1.4.0"
  }
}
