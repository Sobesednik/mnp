{
  "name": "mnp",
  "version": "0.8.0",
  "description": "Quickly create a Node.js package with a Github repository",
  "main": "build",
  "bin": "build/bin/index.js",
  "scripts": {
    "t": "zoroaster -b",
    "test": "yarn t test/spec",
    "test-build": "BABEL_ENV=test-build yarn test",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "dev": "node src/bin/register",
    "start": "node build/bin",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "build": "yarn-s b doc",
    "b": "b --ignore src/bin/register.js --source-maps"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artdecocode/mnp.git"
  },
  "keywords": [
    "npm",
    "package",
    "create",
    "github",
    "git",
    "repo",
    "repository"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/mnp/issues"
  },
  "homepage": "https://github.com/artdecocode/mnp#readme",
  "devDependencies": {
    "assert-diff": "1.2.6",
    "documentary": "1.7.0",
    "eslint-config-artdeco": "1.0.0",
    "snapshot-context": "2.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "2.1.0"
  },
  "dependencies": {
    "africa": "1.1.1",
    "argufy": "1.2.1",
    "camel-case": "3.0.0",
    "erte": "1.1.4",
    "makepromise": "2.0.0",
    "mnp-idio": "1.2.5",
    "mnp-package": "1.4.0",
    "mnp-structure": "1.0.0",
    "promto": "1.0.1",
    "reloquent": "1.0.1",
    "restream": "2.1.0",
    "rqt": "2.0.0",
    "spawncommand": "2.0.1",
    "usually": "1.0.0",
    "wrote": "1.4.0"
  }
}
