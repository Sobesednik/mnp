{
  "name": "mnp",
  "version": "0.6.1",
  "description": "Quickly create a Node.js package with a Github repository",
  "main": "build",
  "bin": "build/bin/mnp.js",
  "scripts": {
    "t": "zoroaster --babel",
    "test": "yarn t test/spec",
    "test-build": "BABEL_ENV=test-build yarn test",
    "test-watch": "yarn test --watch",
    "lint": "eslint .",
    "dev": "node src/bin",
    "build": "babel src --out-dir build --ignore src/bin/index.js"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artdecocode/mnp.git"
  },
  "keywords": [
    "npm",
    "package",
    "create",
    "github",
    "git",
    "repo",
    "repository"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/mnp/issues"
  },
  "homepage": "https://github.com/artdecocode/mnp#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.47",
    "@babel/core": "7.0.0-beta.47",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.47",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.47",
    "@babel/register": "7.0.0-beta.47",
    "assert-diff": "1.2.6",
    "eslint": "4.19.1",
    "eslint-config-artdeco": "1.0.0",
    "zoroaster": "2.0.0"
  },
  "dependencies": {
    "africa": "1.0.1",
    "camel-case": "3.0.0",
    "makepromise": "2.0.0",
    "mnp-idio": "1.2.5",
    "mnp-package": "1.1.1",
    "mnp-structure": "1.0.0",
    "reloquent": "0.2.0",
    "restream": "2.0.1",
    "rqt": "1.1.0",
    "spawncommand": "2.0.1",
    "wrote": "1.4.0"
  }
}
