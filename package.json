{
  "name": "mnp",
  "version": "0.6.1",
  "description": "Quickly create a Node.js package with a Github repository",
  "main": "src/index.js",
  "bin": "./bin/mnp",
  "scripts": {
    "test": "zoroaster test/spec",
    "test-watch": "zoroaster test/spec --watch",
    "postpublish": "git push --follow-tags"
  },
  "files": [
    "bin",
    "src",
    "structures/my-new-package"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artdecocode/mnp.git"
  },
  "keywords": [
    "npm",
    "package",
    "create",
    "github",
    "git",
    "repo",
    "repository"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecocode/mnp/issues"
  },
  "homepage": "https://github.com/artdecocode/mnp#readme",
  "devDependencies": {
    "assert-diff": "1.2.6",
    "zoroaster": "2.0.0"
  },
  "dependencies": {
    "africa": "1.0.1",
    "camel-case": "3.0.0",
    "makepromise": "2.0.0",
    "mnp-idio": "1.2.5",
    "mnp-package": "1.1.1",
    "mnp-structure": "1.0.0",
    "reloquent": "0.2.0",
    "restream": "2.0.1",
    "rqt": "1.1.0",
    "spawncommand": "2.0.1",
    "wrote": "1.4.0"
  }
}
