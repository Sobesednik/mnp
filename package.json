{
  "name": "mnp",
  "version": "0.14.0",
  "description": "Quickly create a Node.js package with a Github repository.",
  "main": "build",
  "bin": {
    "mnp": "build/bin/mnp.js",
    "mnp-dev": "src/bin/index.js"
  },
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask -t 20000",
    "spec": "yarn t test/spec -t 20000",
    "mask": "yarn t test/mask -t 20000",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "d": "git add documentary README.md; git commit -m doc; git push;",
    "dev": "node src/bin",
    "start": "node build/bin/mnp",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "b": "alamode src -o build -i bin/index.js,bin/.eslintrc -s",
    "build": "yarn-s b doc"
  },
  "files": [
    "build",
    "src/bin/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/dpck/depack.git"
  },
  "keywords": [
    "npm",
    "mnp",
    "new",
    "node",
    "node.js",
    "package",
    "create",
    "github",
    "git",
    "repo",
    "repository",
    "bootstrap",
    "scaffold",
    "development",
    "dev",
    "generate",
    "generator",
    "yeoman",
    "slush",
    "project",
    "init",
    "template",
    "modern"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mnpjs/mnp/issues"
  },
  "homepage": "https://github.com/mnpjs/mnp#readme",
  "devDependencies": {
    "@wrote/read": "^1.0.4",
    "@wrote/rm": "^1.1.4",
    "alamode": "^1.9.3",
    "assert-diff": "2.0.3",
    "documentary": "^1.24.1",
    "eslint-config-artdeco": "1.0.1",
    "temp-context": "^2.1.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^3.11.6"
  },
  "dependencies": {
    "@mnpjs/azure": "^1.0.1",
    "@mnpjs/idio": "^1.3.1",
    "@mnpjs/package": "^1.8.0",
    "@mnpjs/structure": "^1.1.1",
    "@rqt/github": "^1.0.0",
    "africa": "^1.2.0",
    "argufy": "^1.5.0",
    "bosom": "^1.0.0",
    "camel-case": "^3.0.0",
    "erte": "^1.1.7",
    "makepromise": "^3.1.0",
    "promto": "^1.0.1",
    "reloquent": "^1.2.4",
    "restream": "^3.6.0",
    "rqt": "^3.1.3",
    "spawncommand": "^2.1.2",
    "usually": "^1.2.0",
    "wrote": "^1.4.0"
  }
}
