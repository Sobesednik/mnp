{
  "name": "mnp",
  "version": "0.4.2",
  "description": "Quickly create a Node.js package with a Github repository Edit",
  "main": "src/index.js",
  "bin": "./bin/mnp",
  "scripts": {
    "test": "zoroaster test/spec",
    "test-watch": "zoroaster test/spec --watch"
