{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach",
      "port": 9230
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Dev",
      "program": "${workspaceFolder}/src/bin/alamode.js",
      "env": {
        "BABEL_ENV": "debug",
        "NODE_DEBUG": "mnp",
      },
      "cwd": "${workspaceFolder}/../../mnp-test",
      "args": [
        "debug-package-1", // package-name
        // "-d", // delete github
        // "-s", "structure",
        // "-l", // local
        // "test", "-c",
        // "-c", // check
        // "-l", // local
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch mnp",
      "program": "${workspaceFolder}/build/bin/index.js",
      "env": {
        "NODE_DEBUG": "mnp,aqt",
      },
      "cwd": "${workspaceFolder}/../../mnpjs",
      "args": [
        "package8", // package-name
        "-s", "structure", // structure
        "-d", // delete
        // "-l", // local
        // "-c", // check
        // "-l", // local
      ],
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Zoroaster",
      "program": "${workspaceFolder}/node_modules/.bin/zoroaster",
      "args": [
        "test/spec",
        "test/mask",
        "-a",
        "-w",
        "-t",
        "9999999"
      ],
      "env": {
        "NODE_DEBUG": "mnp,aqt",
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
